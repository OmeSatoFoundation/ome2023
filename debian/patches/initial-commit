+/* Define if you prefer addlog array function */
+#define USE_ADDLOG_ARRAY 1
+
+/* Define if you use zcat command for compressed file input */
+/* #undef ZCAT */
+
+/* Define if you have socklen definition */
+#define HAVE_SOCKLEN_T 1
+
